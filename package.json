{
  "name": "svelte-date-picker",
  "version": "0.0.1",
  "scripts": {
    "build": "rollup -c",
    "dev": "run-p dev:*",
    "dev:rollup": "rollup -c -w",
    "dev:tailwind": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "start": "sirv public --no-clear -s --port 5556"
  },
  "author": "Baskara",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "sirv-cli": "^2.0.0"
  }
}
